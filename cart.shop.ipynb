    "store='頂好購物'\n",
    "products=['電池','洗碗精','洗衣粉','衣架','掃把','拖把','烤肉夾']\n",
    "cart=[]\n",
    "print(store)\n",
    "print(products,'\\n')\n",
    "while True:\n",
    "    msg=input(\"輸入要買的商品(不買就按Q):\")\n",
    "    if msg=='q' or msg=='Q':\n",
    "        break\n",
    "    else:\n",
    "        if msg in products:\n",
    "            cart.append(msg)\n",
    "print(\"剛剛下單的物品有:\", cart)"
